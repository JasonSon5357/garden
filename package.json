{
	"name": "garden",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev:dash": "next dev -p 3000",
		"dev:static": "concurrently \"esbuild ./static-server/index.ts --watch --bundle --platform=node --target=node16 --external:'./node_modules/*' --outfile=./static-server/index.js\" \"nodemon --watch ./static-server/index.js ./static-server/index.js\"",
		"dev": "concurrently \"yarn dev:dash\" \"yarn dev:static\"",
		"lint:dash": "next lint",
		"build:dash": "next build",
		"build:static": "esbuild ./static-server/index.ts --bundle --platform=node --target=node16 --external:'./node_modules/*' --outfile=./static-server/index.js",
		"build": "concurrently \"yarn build:dash\" \"yarn build:static\"",
		"start:dash": "next start -p 3000",
		"start:static": "node ./static-server/index.js",
		"start": "concurrently \"yarn start:dash\" \"yarn start:static\""
	},
	"dependencies": {
		"@geist-ui/core": "^2.3.8",
		"@geist-ui/icons": "^1.0.1",
		"@prisma/client": "3.12.0",
		"axios": "^0.26.1",
		"capture-website": "^2.3.0",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"express": "^4.17.3",
		"get-folder-size": "^3.1.0",
		"middleware-async": "^1.3.2",
		"next": "12.1.4",
		"next-validations": "^0.2.0",
		"pretty-bytes": "^6.0.0",
		"react": "18.0.0",
		"react-dom": "18.0.0",
		"react-hook-form": "^7.29.0",
		"react-query": "4.0.0-beta.2",
		"recursive-readdir": "^2.2.2",
		"sharp": "^0.30.3",
		"superjson": "^1.8.1",
		"zod": "^3.14.4"
	},
	"devDependencies": {
		"@types/cookie-parser": "^1.4.2",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/get-folder-size": "^3.0.1",
		"@types/node": "17.0.23",
		"@types/react": "18.0.0",
		"@types/react-dom": "18.0.0",
		"@types/react-timeago": "^4.1.3",
		"@types/recursive-readdir": "^2.2.0",
		"autoprefixer": "^10.4.4",
		"concurrently": "^7.1.0",
		"esbuild": "^0.14.34",
		"eslint": "8.13.0",
		"eslint-config-next": "12.1.4",
		"eslint-plugin-unused-imports": "^2.0.0",
		"nodemon": "^2.0.15",
		"postcss": "^8.4.12",
		"prettier": "^2.6.2",
		"prettier-plugin-tailwindcss": "^0.1.8",
		"prisma": "^3.12.0",
		"tailwindcss": "^3.0.23",
		"typescript": "4.6.3"
	}
}
